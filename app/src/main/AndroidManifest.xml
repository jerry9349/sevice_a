<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jerry.sevice_a">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <!--맨 첫화면을 .CSignUpActivity1가 아닌 MainActivity로 바꾸면 첫 화면이 Main화면으로 뜸-->
        <activity android:name=".CSignUpActivity1">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CSignUpActivity2" />
        <activity android:name=".CSignUpActivity3" />
        <activity android:name=".MainActivity" />
        <activity android:name=".OrderActivity" />
    </application>

</manifest>